{
  "containerDefinitions": [
    {
      "name": "resonant",
      "image": "211125331632.dkr.ecr.us-east-1.amazonaws.com/resonant:prod",
      "cpu": 512,
      "memory": 1024,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "NEXT_BASE_URL",
          "value": "https://app.resonantdx.com"
        },
        {
          "name": "PORT",
          "value": "8080"
        },
        {
          "name": "ENVIRONMENT",
          "value": "prod"
        },
        {
          "name": "HOST",
          "value": "0.0.0.0"
        },
        {
          "name": "SMTP_HOST",
          "value": "email-smtp.us-west-2.amazonaws.com"
        },
        {
          "name": "SMTP_PORT",
          "value": "587"
        },
        {
          "name": "SMTP_FROM",
          "value": "noreply@resonantdx.com"
        },
        {
          "name": "INTEGRATION_WBL_URL",
          "value": "https://dev.wasatchbiolabs.com/api/integrations/resonant"
        },
        {
          "name": "S3_REGION",
          "value": "us-east-1"
        },
        {
          "name": "S3_REQUISITION_FORM_BUCKETNAME",
          "value": "prod-resonant-requisition-forms"
        },
        {
          "name": "S3_MEDICAL_LICENSE_BUCKETNAME",
          "value": "prod-resonant-medical-licenses"
        },
        {
          "name": "S3_GOVERNMENT_ID_BUCKETNAME",
          "value": "prod-resonant-government-ids"
        },
        {
          "name": "S3_RESULTS_BUCKETNAME",
          "value": "prod-resonant-results"
        },
        {
          "name": "S3_UPLOADS_BUCKETNAME",
          "value": "prod-resonant-uploads"
        }
      ],
      "secrets": [
        {
          "name": "DATABASE_URL",
          "valueFrom": "arn:aws:ssm:us-east-1:211125331632:parameter/prod/rds/resonant/url"
        },
        {
          "name": "NEXTAUTH_SECRET",
          "valueFrom": "arn:aws:ssm:us-east-1:211125331632:parameter/prod/services/resonant/env-vars/NEXTAUTH_SECRET"
        },
        {
          "name": "SMTP_USER",
          "valueFrom": "arn:aws:ssm:us-east-1:211125331632:parameter/prod/services/resonant/env-vars/SMTP_USER"
        },
        {
          "name": "SMTP_PASSWORD",
          "valueFrom": "arn:aws:ssm:us-east-1:211125331632:parameter/prod/services/resonant/env-vars/SMTP_PASSWORD"
        },
        {
          "name": "CRON_API_KEY",
          "valueFrom": "arn:aws:ssm:us-east-1:211125331632:parameter/prod/services/resonant/env-vars/CRON_API_KEY"
        },
        {
          "name": "INTEGRATION_WBL_VALIDATION_TOKEN",
          "valueFrom": "arn:aws:ssm:us-east-1:211125331632:parameter/prod/services/resonant/env-vars/INTEGRATION_WBL_VALIDATION_TOKEN"
        },
        {
          "name": "INTEGRATION_RESONANT_REQUEST_TOKEN",
          "valueFrom": "arn:aws:ssm:us-east-1:211125331632:parameter/prod/services/resonant/env-vars/INTEGRATION_RESONANT_REQUEST_TOKEN"
        },
        {
          "name": "RETOOL_AUTH",
          "valueFrom": "arn:aws:ssm:us-east-1:211125331632:parameter/prod/services/resonant/env-vars/RETOOL_AUTH"
        },
        {
          "name": "STRIPE_PRIVATE_KEY",
          "valueFrom": "arn:aws:ssm:us-east-1:211125331632:parameter/prod/services/resonant/env-vars/STRIPE_PRIVATE_KEY"
        },
        {
          "name": "STRIPE_KIT_PRODUCT_ID",
          "valueFrom": "arn:aws:ssm:us-east-1:211125331632:parameter/prod/services/resonant/env-vars/STRIPE_KIT_PRODUCT_ID"
        },
        {
          "name": "STRIPE_KIT_PRICE_ID",
          "valueFrom": "arn:aws:ssm:us-east-1:211125331632:parameter/prod/services/resonant/env-vars/STRIPE_KIT_PRICE_ID"
        },
        {
          "name": "STRIPE_WEBHOOK_ENDPOINT_KEY",
          "valueFrom": "arn:aws:ssm:us-east-1:211125331632:parameter/prod/services/resonant/env-vars/STRIPE_WEBHOOK_ENDPOINT_KEY"
        }
      ],
      "mountPoints": [],
      "volumesFrom": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/aws/ecs/prod-resonant-cluster/resonant",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "resonant"
        }
      }
    }
  ],
  "taskRoleArn": "arn:aws:iam::211125331632:role/prod-resonant-cluster-resonant-task-role",
  "executionRoleArn": "arn:aws:iam::211125331632:role/prod-resonant-cluster-resonant-exec-role",
  "networkMode": "awsvpc",
  "family": "prod-resonant-cluster-resonant",
  "volumes": [],
  "cpu": "512",
  "memory": "1024",
  "placementConstraints": [],
  "requiresCompatibilities": ["FARGATE"],
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  }
}
